<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true"/>

        <service id="sylius.shop_api_plugin.factory.customer_view_factory"
                 class="Sylius\ShopApiPlugin\Factory\Customer\CustomerViewFactory">
            <argument type="string">%sylius.shop_api.view.customer.class%</argument>
            <argument type="service" id="sylius.shop_api_plugin.factory.image_view_factory"/>
        </service>
    </services>
</container>
